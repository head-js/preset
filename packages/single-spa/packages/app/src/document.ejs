<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1,minimum-scale=1,user-scalable=no" />
    <link rel="icon" href="/rsrc/img/favicon.ico" />
    <title>@head/preset-vue</title>
    <% htmlWebpackPlugin.options.headScripts.forEach(function (script) { %>
      <%= script %>
    <% }); %>
    <style>
      body { margin: 0 }
    </style>
  </head>
  <body>
    <div id="root" class="RootApp" data-pathname="/"></div>
    <script src="/system.js"></script>
    <script>
      System.addImportMap({
        imports: {
          'react-app': 'http://127.0.0.1:8001/.dist/umi.js',
          'vue-app': 'http://127.0.0.1:8001/.dist/vue.js',
        },
      });
      System.import('/js/umi.js');
    </script>
  </body>
</html>
